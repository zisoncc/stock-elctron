{
  "name": "stock-ticker",
  "version": "1.0.0",
  "description": "股票基金行情显示软件",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . stock-ticker --platform=win32 --arch=x64 --out=release-builds --overwrite",
    "package-mac": "electron-packager . stock-ticker --platform=darwin --arch=x64 --out=release-builds --overwrite",
    "package-linux": "electron-packager . stock-ticker --platform=linux --arch=x64 --out=release-builds --overwrite",
    "build": "electron-builder",
    "build-win": "electron-builder --win"
  },
  "keywords": [
    "股票",
    "基金",
    "行情",
    "Electron"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-packager": "^17.1.2"
  }
}
